@{
    ViewData["Title"] = "SuccessPayment";
}

<!-- Single Page Header start -->
<div class="container-fluid page-header py-5">
    <h1 class="text-center text-white display-6">Cart</h1>
    <ol class="breadcrumb justify-content-center mb-0">
        <li class="breadcrumb-item"><a href="#">Home</a></li>
        <li class="breadcrumb-item"><a href="#">Pages</a></li>
        <li class="breadcrumb-item active text-white">Cart</li>
    </ol>
</div>
<!-- Single Page Header End -->
<!-- Featurs Section Start -->
<div class="container-fluid featurs py-5 row justify-content-center">
    <div class="container py-5 row justify-content-center text-center">
        <div class=" mb-5 order-placed ">
            <img src="~/img/ordersuccess.jpg" class="img-fluid" />
        </div>
        <h1 class="mt-5">Order <span style="color:red">Placed</span></h1>
        <a asp-controller="Sellers" asp-action="Sellers" class="btn btn-primary border-2 border-secondary  rounded-pill text-white w-25 mt-2">Back to Shop</a>
    </div>
</div>




